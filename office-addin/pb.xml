<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
  <Id>e27e6734-3378-4e1b-bf70-90498ceac862</Id>
  <Version>1.1.0.2</Version>
  <ProviderName>Phished</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Phished"/>
  <Description DefaultValue="Report suspicious mails to Phished.">
    <Override Locale="nl-BE" Value="Rapporteer verdachte mails aan Phished"/>
  </Description>

  <!-- JOUW ICONEN -->
  <IconUrl DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-128.png"/>

  <SupportUrl DefaultValue="https://phished.io/contact">
    <Override Locale="nl-BE" Value="https://phished.io/nl-be/contact"/>
  </SupportUrl>
  <AppDomains>
    <AppDomain>https://outlook-2024.phished.io</AppDomain>
    <!--  MSAL domains  -->
    <AppDomain>https://login.microsoftonline.com</AppDomain>
    <AppDomain>https://login.live.com</AppDomain>
    <AppDomain>https://smtp.office365.com</AppDomain>
    <AppDomain>https://outlook.office365.com</AppDomain>
    <AppDomain>https://smtp-mail.outlook.com</AppDomain>
    <AppDomain>https://outlook.office.com</AppDomain>
    <AppDomain>https://login.windows.net</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://outlook-2024.phished.io/taskpane.html"/>
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteMailbox</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
  </Rule>

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
      <Requirements>
        <bt:Sets DefaultMinVersion="1.3">
          <bt:Set Name="Mailbox"/>
        </bt:Sets>
      </Requirements>
      <Hosts>
        <Host xsi:type="MailHost">
          <DesktopFormFactor>
            <SupportsSharedFolders>true</SupportsSharedFolders>
            <ExtensionPoint xsi:type="MessageReadCommandSurface">
              <OfficeTab id="TabDefault">
                <Group id="msgComposeGroup">
                  <Label resid="GroupLabel"/>
                  <Control xsi:type="Button" id="msgComposeOpenPaneButton">
                    <Label resid="TaskpaneButton.Label"/>
                    <Supertip>
                      <Title resid="TaskpaneButton.Label"/>
                      <Description resid="TaskpaneButton.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                      <SourceLocation resid="Taskpane.Url"/>
                    </Action>
                  </Control>
                </Group>
              </OfficeTab>
            </ExtensionPoint>
          </DesktopFormFactor>
          <MobileFormFactor>
            <ExtensionPoint xsi:type="MobileMessageReadCommandSurface">
              <Group id="mobileMsgReadGroup">
                <Label resid="GroupLabel"/>
                <Control xsi:type="MobileButton" id="MobileReportButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Icon xsi:type="bt:MobileIconList">
                    <bt:Image resid="Icon.25x25" size="25" scale="1"/>
                    <bt:Image resid="Icon.50x50" size="25" scale="2"/>
                    <bt:Image resid="Icon.75x75" size="25" scale="3"/>
                    <bt:Image resid="Icon.32x32" size="32" scale="1"/>
                    <bt:Image resid="Icon.64x64" size="32" scale="2"/>
                    <bt:Image resid="Icon.96x96" size="32" scale="3"/>
                    <bt:Image resid="Icon.48x48" size="48" scale="1"/>
                    <bt:Image resid="Icon.96x96" size="48" scale="2"/>
                    <bt:Image resid="Icon.144x144" size="48" scale="3"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </ExtensionPoint>
          </MobileFormFactor>
        </Host>
      </Hosts>

      <Resources>
        <bt:Images>
          <bt:Image id="Icon.16x16"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-16.png"/>
          <bt:Image id="Icon.25x25"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-25.png"/>
          <bt:Image id="Icon.32x32"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-32.png"/>
          <bt:Image id="Icon.48x48"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-48.png"/>
          <bt:Image id="Icon.50x50"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-50.png"/>
          <bt:Image id="Icon.64x64"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-64.png"/>
          <bt:Image id="Icon.75x75"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-75.png"/>
          <bt:Image id="Icon.80x80"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-80.png"/>
          <bt:Image id="Icon.96x96"  DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-96.png"/>
          <bt:Image id="Icon.144x144" DefaultValue="https://cyberavant.nl/wp-content/uploads/2025/11/Button-144.png"/>
        </bt:Images>

        <bt:Urls>
          <bt:Url id="Taskpane.Url" DefaultValue="https://outlook-2024.phished.io/taskpane.html"/>
        </bt:Urls>

        <bt:ShortStrings>
          <bt:String id="GroupLabel" DefaultValue="Report"/>
          <bt:String id="TaskpaneButton.Label" DefaultValue="Report">
            <bt:Override Locale="nl-BE" Value="Rapporteer"/>
          </bt:String>
        </bt:ShortStrings>

        <bt:LongStrings>
          <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Report this mail as suspicious to Phished.">
            <bt:Override Locale="nl-BE" Value="Rapporteer deze mail als verdacht aan Phished."/>
          </bt:String>
        </bt:LongStrings>
      </Resources>

      <WebApplicationInfo>
        <Id>f648076c-88b9-44fd-9df6-7a42919f71ed</Id>
        <Resource>api://outlook-2024.phished.io/f648076c-88b9-44fd-9df6-7a42919f71ed</Resource>
        <Scopes>
          <Scope>profile</Scope>
          <Scope>openid</Scope>
          <Scope>User.Read</Scope>
          <Scope>Mail.Read</Scope>
          <Scope>Mail.Read.Shared</Scope>
          <Scope>Mail.ReadBasic</Scope>
          <Scope>Mail.ReadBasic.Shared</Scope>
          <Scope>Mail.ReadWrite</Scope>
          <Scope>Mail.ReadWrite.Shared</Scope>
        </Scopes>
      </WebApplicationInfo>
    </VersionOverrides>
  </VersionOverrides>
</OfficeApp>
